<div class="dialog-overlay d-block">
    <div class="dialog">
        <div class="dialog-header">
            <i class="icon-warning-sign"></i>

            <h3>${config.title}</h3>
        </div>

        <div class="dialog-content">
            <p>
                ${config.message}
            </p>



            <a class="button confirm right" href="${config.confirmHref}">Confirm</a>
            <a class="button cancel" href="${config.cancelHref}">Cancel</a>
        </div>
    </div>
</div>