<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.cdss.api.data">

    <class name="EngineUsage" table="cdss_usage">

        <id name="id" type="java.lang.Integer">
            <column name="cdss_usage_id"/>
            <generator class="identity"/>
        </id>

        <property name="vaccine" type="java.lang.String">
            <column name="cdss_usage_vaccine" length="255" not-null="true"/>
        </property>

        <many-to-one name="patient" class="org.openmrs.Patient" column="patient_id" not-null="true"/>

        <property name="timestamp" type="org.joda.time.DateTime">
            <column name="cdss_usage_date" not-null="true"/>
        </property>

    </class>

</hibernate-mapping>