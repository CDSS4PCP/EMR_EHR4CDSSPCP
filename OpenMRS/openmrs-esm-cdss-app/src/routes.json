{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2.0",
    "webservices.rest": ">=2.24.0",
    "cdss": "1.0.0-ALPHA"
  },
  "extensions": [
    {
      "name": "cdss-chart",
      "component": "cdssChart",
      "slot": "patient-chart-cdss-dashboard-slot",
      "meta": {
        "columnSpan": 4
      },
      "order": 8
    },
    {
      "name": "cdss-summary-dashboard",
      "component": "cdssDashboardLink",
      "slot": "patient-chart-dashboard-slot",
      "meta": {
        "slot": "patient-chart-cdss-dashboard-slot",
        "columns": 1,
        "path": "Cdss"
      },
      "order": 12
    },
    {
      "name": "cdss-reports-link",
      "slot": "app-menu-slot",
      "component": "cdssReportsLink",
      "online": true,
      "offline": true
    },
    {
      "name": "cdss-management-link",
      "slot": "app-menu-slot",
      "component": "cdssManagementLink",
      "online": true,
      "offline": true
    },
    {
      "name": "cdss-archive-link",
      "slot": "app-menu-slot",
      "component": "cdssArchiveLink",
      "online": true,
      "offline": true
    }
  ],
  "pages": [
    {
      "component": "cdssReportsPage",
      "route": "cdss/reports",
      "online": true,
      "offline": true
    },
    {
      "component": "cdssModificationPage",
      "route": "cdss/edit",
      "online": true,
      "offline": true
    },
    {
      "component": "cdssArchivePage",
      "route": "cdss/archive",
      "online": true,
      "offline": true
    }
  ]
}
